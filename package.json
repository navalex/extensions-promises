{
  "name": "extensions",
  "version": "1.1.0",
  "repositoryName": "Navalex's Extensions",
  "description": "French manga sources for Paperback IOS App",
  "scripts": {
    "start": "tsc && node dist/api.js",
    "build": "tsc",
    "test": "mocha --timeout 300000 -r ts-node/register src/**/*.test.ts",
    "coverage": "nyc -r lcov -e .ts -x \"*.test.ts\" npm run test",
    "bundle": "paperback bundle",
    "serve": "paperback serve -p 8000"
  },
  "author": "Navalex",
  "license": "MIT",
  "dependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cheerio": "^1.0.0-rc.10",
    "paperback-cli": "^1.4.0",
    "paperback-extensions-common": "^4.3.5",
    "ts-mocha": "^7.0.0",
    "tsify": "^4.0.2",
    "typescript": "^3.9.6"
  },
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/chai-as-promised": "^7.1.4",
    "@types/cheerio": "^0.22.21",
    "@types/mocha": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "eslint": "^7.18.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.2.0",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "typedoc": "^0.22.15"
  }
}
